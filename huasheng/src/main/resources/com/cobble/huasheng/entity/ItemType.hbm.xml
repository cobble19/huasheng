<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cobble.huasheng.entity">
	<class name="ItemTypeEntity" table="B_ITEM_TYPE">
		<id name="itemTypeId" column="ITEM_TYPE_ID">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="NAME"></property>
		<set name="itemBaseInfoEntities" table="R_ITEM_BASE_INFO_ITEM_TYPE" inverse="true">
			<key column="ITEM_TYPE_ID" />
			<many-to-many class="ItemBaseInfoEntity" column="ITEM_BASE_INFO_ID"/>
		</set>
	</class>
</hibernate-mapping>