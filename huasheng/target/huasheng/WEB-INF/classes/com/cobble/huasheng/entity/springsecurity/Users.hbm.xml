<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cobble.huasheng.entity.springsecurity">
	<class name="UsersEntity" table="USERS">
		<id name="username" column="USERNAME">
		</id>
		<property name="password" column="PASSWORD" not-null="true"></property>
		<property name="enabled" column="ENABLED" not-null="true"></property>
	</class>
</hibernate-mapping>