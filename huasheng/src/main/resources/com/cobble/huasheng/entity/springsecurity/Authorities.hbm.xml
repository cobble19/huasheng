<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cobble.huasheng.entity.springsecurity">
	<class name="AuthoritiesEntity" table="AUTHORITIES">
		<id name="authorityId" column="AUTHORITY_ID">
		</id>
		<property name="authority" column="AUTHORITY" not-null="true"></property>
		<property name="username" column="USERNAME" not-null="true"></property>
	</class>
</hibernate-mapping>