<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cobble.huasheng.entity">
	<class name="VideoEntity" table="B_VIDEO">
		<id name="videoId" column="VIDEO_ID">
			<generator class="increment"></generator>
		</id>
		<property name="url" column="URL" not-null="true"></property>
		<property name="orderNum" column="ORDER_NUM" not-null="true"></property>
		<many-to-one name="itemEntity" column="ITEM_ID" cascade="delete"></many-to-one>
		<many-to-one name="videoSrcEntity" column="VIDEO_SRC_ID"></many-to-one>
	</class>
</hibernate-mapping>